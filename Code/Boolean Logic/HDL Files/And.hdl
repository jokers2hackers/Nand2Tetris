/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    // NAND gate implementation assumed to be built-in.
    // NOT gate implementation done in Not.hdl
    Nand (a = a, b = b, out = nandab);
    Not(in = nandab, out = out);
}